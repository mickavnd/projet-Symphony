{% extends "base.html.twig" %}

{% block title %}
	creation d'une category
{% endblock %}

{% block body %}
	<h1>créer une category</h1>
	{{form_start(formView)}}

	{{form_errors(formView)}}

	<div class="row">
		<div class="col">
			{{form_row(formView.name)}}
		</div>
	</div>
    <button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		crée le produit
	</button>
{{ form_end(formView)}}
{% endblock %}
